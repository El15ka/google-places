{
    "actorSpecification": 1,
    "title": "Google Places Dataset",
    "description": "",
    "views": {
        "places": {
            "title": "Google Places search results",
            "description": "It can take about one minute until the first results are available.",
            "transformation": {
                "fields": [
                    "title",
                    "categoryName",
                    "address",
                    "street",
                    "url",
                    "city",
                    "state",
                    "countryCode",
                    "website",
                    "phone",
                    "totalScore",
                    "reviewsCount"
                ]
            },
            "display": {
                "component": "table",
                "columns": [
                    {
                        "label": "Category",
                        "format": "text",
                        "field": "$categoryName"
                    },
                    {
                        "label": "Place name",
                        "format": "link",
                        "field": "$url",
                        "textField": "$title"
                    },
                    {
                        "label": "Total score",
                        "format": "text",
                        "field": "$totalScore"
                    },
                    {
                        "label": "Reviews count",
                        "format": "text",
                        "field": "$reviewsCount"
                    },
                    {
                        "label": "Street",
                        "format": "text",
                        "field": "$street"
                    },
                    {
                        "label": "City",
                        "format": "text",
                        "field": "$city"
                    },
                    {
                        "label": "State",
                        "format": "text",
                        "field": "$state"
                    },
                    {
                        "label": "Country code",
                        "format": "text",
                        "field": "$countryCode"
                    },
                    {
                        "label": "Website",
                        "format": "link",
                        "field": "$website",
                        "textField": "$website"
                    },
                    {
                        "label": "Phone",
                        "format": "text",
                        "field": "$phone"
                    }
                ]
            }
        }
    }
}